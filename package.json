{
  "name": "gadget-catalog",
  "version": "0.19.2",
  "description": "A react application for cataloging gadgets",
  "author": "Shibbir Ahmed",
  "license": "MIT",
  "scripts": {
    "start": "run-p webpack:development pm2:development",
    "pm2:development": "cross-env NODE_ENV=development pm2 start server.js --watch --no-daemon --name gadget-catalog --ignore-watch public/uploads",
    "webpack:development": "cross-env NODE_ENV=development webpack --progress --watch --stats normal",
    "pm2:production": "cross-env NODE_ENV=production pm2 start server.js --name gadget-catalog",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress",
    "production": "run-s webpack:production pm2:production",
    "test": "jest --detectOpenHandles",
    "seed": "node seeder.js",
    "eslint": "eslint src/**/*.js"
  },
